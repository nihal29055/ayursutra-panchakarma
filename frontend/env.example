# AyurSutra Frontend Environment Configuration
# Copy this file to .env and update the values

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api/v1
REACT_APP_API_TIMEOUT=10000

# Application Configuration
REACT_APP_NAME=AyurSutra
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Panchakarma Patient Management & Therapy Scheduling Software

# Environment
REACT_APP_ENV=development

# External Services
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
REACT_APP_ANALYTICS_ID=your-analytics-id

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_PWA=false
REACT_APP_ENABLE_OFFLINE_MODE=false

# UI Configuration
REACT_APP_THEME=light
REACT_APP_PRIMARY_COLOR=#2E7D32
REACT_APP_SECONDARY_COLOR=#FF6F00

# Development
REACT_APP_DEBUG=true
REACT_APP_MOCK_API=false
